<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="authenticator" namespace="/auth" extends="struts-default">
        <action name="auth_*" method="{1}"
            class="com.tansun.rdp4j.generalapps.user.action.AuthenticatorAction">
            <result name="welcome" >/pages/rdp4j/common/user/mainpage.jsp</result>
            <result name="logout">/pages/rdp4j/common/user/login.jsp</result>
            <result name="relogin">/pages/rdp4j/common/user/login.jsp</result>
        </action>
    </package>
</struts>
