		   <result name="toTree">/pages/rdp4j/generalapps/reference/reference_tree.jsp</result>
		   <result name="toPage">/pages/rdp4j/generalapps/reference/reference_referenceIframe.jsp</result>
		   <result name="toAdd">/pages/rdp4j/generalapps/reference/reference_add.jsp</result>
		   <result name="toUpdate">/pages/rdp4j/generalapps/reference/reference_update.jsp</result>
		   <result name="addChild">/pages/rdp4j/generalapps/reference/reference_addChild.jsp</result>
		   <result name="success">/pages/rdp4j/generalapps/reference/reference_success.jsp</result>
		</action>
