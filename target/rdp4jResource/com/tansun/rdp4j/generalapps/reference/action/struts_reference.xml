        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="reference" namespace="/reference" extends="default">
		<action name="reference_*" method="{1}" class="com.tansun.rdp4j.generalapps.reference.action.ReferenceAction">
